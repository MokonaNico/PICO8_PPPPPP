--laser

function update_laser()
	mapx=flr(p.x/128)*16
	mapy=flr(p.y/128)*16
	for i=0,15 do
		for j=0,15 do
			tx=mapx+i
			ty=mapy+j
			tile=mget(tx,ty)
			if fget(tile,5) then
				if (time()%2) > 1 then
					if(tile==14)mset(tx,ty,30)
					if(tile==15)mset(tx,ty,31)
				else 
					if(tile==30)mset(tx,ty,14)
					if(tile==31)mset(tx,ty,15)
				end
			end
		end
	end
end
