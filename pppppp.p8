//0 -> menu
//1 -> in game
//2 -> end title
state=0

function _init()
	create_player()
	create_stars()
	init_respawn()
end

function _update()

	if state == 0 then
		update_stars()
		update_menu()
	elseif state == 1 then
	 update_stars()
		update_player()
	elseif state == 2 then
		
	end
	
end

function _draw()
	cls()
	
	if state == 0 then
		draw_menu()
		draw_stars()
	elseif state == 1 then
		
		draw_stars()
		
		palt(0,false)
		mapx=flr(p.x/128)*16
		mapy=flr(p.y/128)*16
		map(mapx,mapy,0,0,16,16)
		palt(0,true)
		
		draw_player()
		


	elseif state == 2 then
		
	end

	
end
