--stars

function create_stars(n)
	stars={}
	for c=1,30 do
		new_star={x=0,y=0,c=0}
		new_star.x=rnd(128)
		new_star.y=rnd(128)
		if c>=20 then
			new_star.c=13
		else
			new_star.c=7
		end
		add(stars,new_star)
	end
end

function update_stars()
	for s in all(stars) do
		if s.c==13 then
			s.x-=1
		else
			s.x-=2
		end
	
		if s.x < 0 then
			s.x=128
			s.y=rnd(128)
		end
	end
end

function draw_stars()
	for s in all(stars) do
		pset(s.x,s.y,s.c)
	end
end
