--energy+ bloc

e_routs={}

function remove_energy(x,y)
	p.can_jump=true
	mapx=flr(x/8)
	mapy=flr(y/8)
	add(e_routs,cocreate(destroy_energy))
	mset(mapx,mapy,0)
	sfx(7)
end

function update_energy()
 col=check_col(p.x,p.y,6)
	if col.c then
		remove_energy(col.x,col.y)
	end
	for r in all(e_routs) do
		coresume(r)
	end
end

function destroy_energy()
	local ex=mapx
	local ey=mapy
	for i=1,60 do
		yield()
	end
	mset(ex,ey,58)
end
