--respawn point handler
last_check={x=0,y=0}

function init_respawn()
	last_check.x = p.x
	last_check.y = p.y
end

function respawn_player()
	p.x=last_check.x
	p.y=last_check.y
	p.sprite=1
	p.grav=1
end

function set_checkpoint()
	last_check.x = p.x - (p.x%8)
	last_check.y = p.y - (p.y%8)
end

