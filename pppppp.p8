function wait(a) 
 for i = 1,a do 
  flip() 
 end 
end

start_time=-1
function update_menu()
	if btn(❎) then 
		sfx(2)
		start_time=time()+0.6
	end
	if start_time != -1 and 
				time() > start_time then
		state=1
	end
end

function draw_menu()
 title="★ pppppp power up! ★"
 start="press ❎ to start"
	print(title,64-#title*2,34,12)
	print(start,64-#start*2,50,12)
end
